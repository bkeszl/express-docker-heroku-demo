{
  "dev": {
    "driver": "mysql",
    "user": {"ENV": "DB_USER"},
    "password": {"ENV": "DB_PASSWORD"},
    "host": {"ENV": "DB_HOST"},
    "database": {"ENV": "DB_NAME"},
    "port": {"ENV": "DB_PORT"},
    "multipleStatements": true,
    "dialect": "mysql"
  },
  
    "pg": {
      "driver": "pg",
        "url":{"ENV": "DATABASE_URL"},
        "addIfNotExists": {
            "ssl": true
        },
        "overwrite": {
            "native": true
        }
    }
,
  "sql-file": true
}